{
  "name": "vertwo/plite",
  "description": "Version2 PHP Framework",
  "version": "dev-0.0.1",
  "type": "library",
  "keywords": [
    "logging",
    "database",
    "aws",
    "local",
    "cli"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Version2",
      "email": "code@v2.dev",
      "homepage": "https://v2.dev/oss"
    }
  ],
  "autoload": {
    "psr-4": {
      "vertwo\\plite\\": "src/plite/"
    },
    "exclude-from-classmap": [
      "**/NOUSE/",
      "**/nouse/",
      "**/Nouse/"
    ],
    "files": [
      "src/plite/constants.php",
      "src/plite/func.php"
    ]
  },
  "require": {
    "ext-json": "*",
    "ext-curl": "*",
    "ext-openssl": "*",
    "ext-pdo": "*",
    "ext-simplexml": "*",
    "2tvenom/cborencode": "1.0.0",
    "phpseclib/phpseclib": "^3.0",
    "aws/aws-sdk-php": "3.128.0",
    "phpmailer/phpmailer": "^6.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  }
}
